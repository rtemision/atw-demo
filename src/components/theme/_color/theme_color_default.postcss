.theme_color_default {
  --color-content-selection: #fff;
  --color-fill-base: #fff;
  --color-fill-selection: #44a452;

  --color-webkit-tap-highlight: rgba(0, 0, 0, 0);

  --color-content-link-typo-base: var(--color-link-view-default-typo-base);
  --color-content-link-typo-hovered: var(--color-link-view-default-typo-hovered);
  --color-content-link-border-base: var(--color-link-view-default-border-base);
  --color-content-link-border-hovered: var(--color-link-view-default-border-hovered);
}
